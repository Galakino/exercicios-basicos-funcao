/*

7. Faça um procedimento que gere e exiba os 20 primeiros termos da série de Fibonacci (Os 
dois primeiros termos da série são 1 e 1, os termos subsequentes são a soma dos dois 
últimos. Observe o exemplo: 1,1,2,3,5,8,13,21,..) 

*/

#include <iostream>
#include <locale.h>
using namespace std;


void fibonacci(){
    int t1 = 1, t2 = 1, calc;
    
    cout<<t1<<" "<<t2<<" ";
    
    for(int i = 3; i <= 20; i++){
        calc = t1 + t2;
        cout<< calc << " ";
        t1 = t2;
        t2 = calc;
    }
    
}

int main()
{
    fibonacci();
    
    
    return 0;
}
